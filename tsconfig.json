{
	"compilerOptions": {
		"outDir": "./docs/html/built",
		"allowJs": true,
		"target": "ES2020",
		"module": "ES2020",
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"alwaysStrict": true,
		"strict": true,
		"noUnusedParameters": false,
		"removeComments": true,
		"moduleResolution": "node"
	},
	"include": [
		"./docs/html/*"
	]
}
